package com.ggbg.note.domain;

import lombok.Builder;
import lombok.Data;

@Data
@Builder
public class AccountDTO {
private int no;
	
	private String email;
	private String name;
	private String password;
	private Role role;
	private String createDate;
	
	public AccountBandEntity toEntity() {
		AccountBandEntity.builder()
	          .email(this.email)
	          .name(this.name)
	          .password(this.password)
	          .build();
	    }
}
